{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "emulator:others": "firebase emulators:start --only auth,firestore,storage",
    "emulator:functions": "firebase emulators:start --only functions --inspect-functions",
    "emulators": "firebase emulators:start --only auth,firestore,storage,functions --inspect-functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^4.6.4"
  },
  "private": true
}
