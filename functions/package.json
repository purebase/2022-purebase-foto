{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "firebase": "npm run build && firebase emulators:start --only auth,firestore,storage,functions --inspect-functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.24.1",
    "googlephotos": "^0.3.5"
  },
  "devDependencies": {
    "typescript": "^4.6.4"
  },
  "private": true
}
